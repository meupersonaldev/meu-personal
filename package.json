{
  "name": "meu-personal",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=web & npm run dev --workspace=api",
    "build": "npm run build --workspaces",
    "start": "npm run start --workspaces",
    "lint": "npm run lint --workspaces",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules",
    "db:push": "npm run prisma:push --workspace=api",
    "db:studio": "npm run prisma:studio --workspace=api"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "node-fetch": "^2.7.0",
    "react-big-calendar": "^1.19.4"
  }
}
